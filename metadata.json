{
	"name": "puppet-etcd",
	"version": "1.12.3",
	"author": "Cristian Falcas and Community",
	"summary": "Installs and configures etcd",
	"license": "Apache-2.0",
	"source": "git@github.com:puppet-etcd/puppet-etcd.git",
	"project_page": "https://github.com/puppet-etcd/puppet-etcd",
	"issues_url": "https://github.com/puppet-etcd/puppet-etcd/issues",
	"requirements": [{
			"name": "puppet",
			"version_requirement": ">=3.4.0 <5.0.0"
