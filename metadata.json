				"7"
			]
		},{
			"operatingsystem": "Scientific",
			"operatingsystemrelease": [ "6",
				"7"
			]
		},{
			"operatingsystem": "Fedora",
			"operatingsystemrelease": [ "24",
				"25",
				"26",
				"27",
				"28",
				"29",
				"30"
			]
		},{
			"operatingsystem": "Debian",
			"operatingsystemrelease": [ "stretch/sid"
			]
		}
